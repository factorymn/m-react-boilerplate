{
  "name": "m-boilerplate",
  "version": "0.0.1",
  "description": "",
  "private": true,
  "author": "Manufactura (http://factory.mn)",
  "contributors": [],
  "scripts": {
    "start": "npm-run-all --parallel start:client start:server",
    "start:client": "webpack-dev-server --config webpack.dev.config.js",
    "start:server": "rm -rf ./.build && webpack --config webpack.dev.server.config.js",
    "prod": "rm -rf .tmp && export NODE_ENV=production && npm-run-all prod:client prod:server && pm2 start .tmp/server.js --name=\"appname\" && pm2 logs appname",
    "prod:client": "webpack --config webpack.prod.config.js",
    "prod:server": "webpack --config webpack.prod.server.config.js",
    "lint": "eslint ./src --quiet",
    "test": "export NODE_ENV=test && jest --coverage",
    "test:watch": "export NODE_ENV=test && jest --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "history": "^4.6.1",
    "lodash": "^4.13.1",
    "material-ui": "^0.20.0",
    "prop-types": "^15.5.8",
    "react": "^16.2.0",
    "react-async-bootstrapper": "^1.1.1",
    "react-async-component": "^1.0.0-beta.3",
    "react-document-title": "^2.0.2",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.0.3",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.2",
    "react-router": "^4.1.1",
    "react-router-config": "^1.0.0-beta.3",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "5.0.0-alpha.6",
    "react-tap-event-plugin": "^3.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.3.6",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "serialize-javascript": "^1.3.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.test.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 98,
        "branches": 91,
        "functions": 98,
        "lines": 98
      }
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "testRegex": "tests/.*\\.test\\.js$",
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTest.js"
  },
  "config": {
    "ghooks": {
      "pre-commit": "eslint ./src --quiet"
    }
  },
  "devDependencies": {
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "react-addons-test-utils": "^15.6.2",
    "autoprefixer-loader": "^3.2.0",
    "babel": "^6.23.0",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.0.0-beta.1",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-system-import-transformer": "^3.1.0",
    "babel-plugin-transform-class-properties": "^6.9.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.12",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.6.1",
    "better-npm-run": "^0.1.0",
    "clean-webpack-plugin": "^0.1.9",
    "copy-webpack-plugin": "^4.0.1",
    "coveralls": "2.13.1",
    "css-loader": "^0.28.0",
    "ejs": "^2.5.5",
    "eslint": "^4.15.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "express": "^4.15.2",
    "express-minify-html": "^0.12.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "ghooks": "^2.0.0",
    "html-webpack-plugin": "^2.22.0",
    "jest-cli": "20.0.4",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.10",
    "redux-immutable-state-invariant": "^2.1.0",
    "start-server-webpack-plugin": "^2.2.0",
    "stats-webpack-plugin": "^0.6.0",
    "style-loader": "^0.19.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "svg-react-loader": "^0.4.5",
    "ua-parser-js": "^0.7.12",
    "url-loader": "^0.6.2",
    "webpack": "3.10.0",
    "webpack-bundle-analyzer": "^2.7.0",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-dev-server": "^2.4.5",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-manifest-plugin": "^1.0.1",
    "webpack-node-externals": "^1.6.0",
    "webpack-vendor-chunk-plugin": "^1.0.0"
  }
}
